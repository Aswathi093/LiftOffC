#include<stdio.h>
#include<string.h>

int main()
{
char s1[100];
char s2[100];
int len1,len2;
printf(" enter 1st string");
gets(s1);
printf("enter 2nd string");
gets(s2);
len1= strlen(s1);
len2= strlen(s2);
for( int i=0;i<=len2;i++)
{
s1[len1+i]=s2[i];
}
printf("%s \n",s1);
int len3=strlen(s1);
for(int j=len3;j>=0;j--)
{ 
printf("%c", s1[j]);
}
return 0;
}
